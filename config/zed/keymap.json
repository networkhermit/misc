[
  {
    "bindings": {
      "ctrl-backspace": "zed::ToggleFullScreen",
      "ctrl-h": "workspace::ToggleLeftDock"
    }
  },
  {
    "bindings": {
      "ctrl-l": "buffer_search::Dismiss"
    },
    "context": "Editor"
  },
  {
    "bindings": {
      "'": [
        "vim::PushOperator",
        {
          "Jump": {
            "line": false
          }
        }
      ],
      "_": "editor::ToggleRelativeLineNumbers",
      "`": [
        "vim::PushOperator",
        {
          "Jump": {
            "line": true
          }
        }
      ],
      "ctrl-j": [
        "workspace::SendKeystrokes",
        "ctrl-w j"
      ],
      "ctrl-k": [
        "workspace::SendKeystrokes",
        "ctrl-w k"
      ],
      "ctrl-shift-,": "zed::OpenSettings",
      "shift-k": "workspace::OpenInTerminal",
      "shift-q": [
        "workspace::SendKeystrokes",
        "shift-z shift-q"
      ],
      "shift-y": [
        "workspace::SendKeystrokes",
        "y $"
      ]
    },
    "context": "vim_mode == normal"
  },
  {
    "bindings": {
      "'": [
        "vim::PushOperator",
        {
          "Jump": {
            "line": false
          }
        }
      ],
      "`": [
        "vim::PushOperator",
        {
          "Jump": {
            "line": true
          }
        }
      ],
      "shift-m": "editor::SortLinesCaseSensitive"
    },
    "context": "vim_mode == visual"
  },
  {
    "bindings": {
      "ctrl-h": null
    },
    "context": "Terminal"
  }
]
